<h2><%= author.name %></h2>
<div>
    <a href="/authors/<%= author.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', {
                url: `/authors/${author.id}`
    }) %>
</div>
<div>
    <% books.forEach(book => { %>
        <img height="150" width="100" src="<%= book.coverImage %>">
    <% }) %>
</div>
